export com.pengyou.model.entity.Admin
    -> package com.pengyou.model.dto.admin

input AdminForRegister {
    username
    password
    email
    phone
    role
    captcha : String
}

specification AdminForLogin {
    username
    password
    email
    phone
}

AdminForLoginView {
    id
    username
    password
    email
    phone
    role
}

AdminForLogout {
    id
}

AdminForVerify {
    email
    phone
}

AdminForDelete {
    ids : MutableList<Long?>
}

input AdminForUpdate {
    id!
    dynamic username?
    dynamic password
    dynamic email
    dynamic phone
}

specification AdminForQuery {
    pageIndex: Int?
    pageSize: Int?
    id
    like/i(username)
    email
    phone
    ge(createdTime)
    le(createdTime)
    ge(modifiedTime)
    le(modifiedTime)
    createdPerson
    modifiedPerson
    role
}

AdminForView {
    id
    username
    email
    phone
    createdTime
    modifiedTime
    createdPerson
    modifiedPerson
    deleteAt
    role
}